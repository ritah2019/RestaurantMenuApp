<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"
        tools:context=".MainActivity">
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="306dp" app:srcCompat="@drawable/gsp"
            android:id="@+id/imageView" android:layout_marginTop="160dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="265dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="28dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="36dp" app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintVertical_bias="0.351" android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true" android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"/>

    <TextView
            android:text="Restaurant"
            android:textStyle="italic"
            android:textAlignment="center"
            android:textSize="30sp"
            android:textColor="#bf1e2e"
            android:layout_width="310dp"
            android:layout_height="83dp" android:layout_alignParentStart="true"
            android:layout_marginStart="35dp" android:id="@+id/textView"
            android:layout_marginBottom="176dp" android:layout_alignParentEnd="true" android:layout_marginEnd="66dp"
            android:layout_alignParentBottom="true"/>
</RelativeLayout>